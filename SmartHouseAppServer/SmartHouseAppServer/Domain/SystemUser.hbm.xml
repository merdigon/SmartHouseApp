<?xml version="1.0" encoding="utf-8"?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" namespace="SmartHouseAppServer.Domain" assembly="SmartHouseAppServer">
  
  <class name="SystemUser" table="system_user">
    <id name="Id">
      <generator class="sequence">
        <param name="sequence">system_user_id_seq</param>
      </generator>
    </id>
    <property name="Mac" column="mac"/>
    <property name="UserWeight" column="user_weight"/>
    <property name="VisibleName" column="visible_name"/>
  </class>
</hibernate-mapping>