<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" namespace="SmartHouseAppServer.Domain" assembly="SmartHouseAppServer">
  <class name="UserPositionHistory" table="user_possition">
    <id name="Id">
      <generator class="sequence">
        <param name="sequence">user_possition_id_seq</param>
      </generator>
    </id>
    <property name="Mac" column="mac"/>
    <property name="X" column="coordinate_x"/>
    <property name="Y" column="coordinate_y"/>
    <property name="Z" column="coordinate_z"/>
    <property name="Date" column="date"/>
  </class>
</hibernate-mapping>

